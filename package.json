{
  "name": "nestjs-full-example",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.11",
    "@nestjs/core": "^9.0.11",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/swagger": "^6.1.0",
    "@opentelemetry/api": "^1.1.0",
    "@opentelemetry/auto-instrumentations-node": "^0.31.2",
    "@opentelemetry/core": "^1.6.0",
    "@opentelemetry/exporter-jaeger": "^1.6.0",
    "@opentelemetry/exporter-prometheus": "^0.32.0",
    "@opentelemetry/host-metrics": "^0.29.0",
    "@opentelemetry/instrumentation-express": "^0.30.0",
    "@opentelemetry/instrumentation-graphql": "^0.29.0",
    "@opentelemetry/instrumentation-nestjs-core": "^0.30.0",
    "@opentelemetry/instrumentation-pino": "^0.30.0",
    "@opentelemetry/instrumentation-winston": "^0.29.0",
    "@opentelemetry/propagator-b3": "^1.6.0",
    "@opentelemetry/propagator-jaeger": "^1.6.0",
    "@opentelemetry/sdk-node": "^0.32.0",
    "@types/graphql": "^14.5.0",
    "aws-sdk": "^2.1205.0",
    "nest-winston": "^1.7.0",
    "nestjs-otel": "^4.0.1",
    "nestjs-pino": "^3.1.1",
    "opentelemetry-node-metrics": "^2.0.0",
    "pino": "^8.4.2",
    "pino-http": "^8.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "swagger-ui-express": "^4.5.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.1",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.11",
    "@opentelemetry/api-metrics": "^0.32.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.13",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.0.0",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
